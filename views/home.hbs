        <!-- /#sidebar-wrapper -->

        <!-- Page Content -->
        <div id="page-content-wrapper">
            <div class="flex-container rm-padding-lr" >
                <div class="row flex-header rm-margin-lr" >{{>header}} </div>
                <div class="row flex-content rm-margin-lr">
                    {{#each product}}
                        <div class="col-xs-6 col-md-4" >
                            <div class="thumbnail">
                                <img src="/images/{{_id}}/{{image}}" alt="no image" class="" >
                                <div class="caption">
                                    <h3>{{name}}</h3>
                                    <p>{{trunc description}}</p>
                                    <p><a href="/cart/product/{{_id}}" class="btn btn-primary" role="button">Buy</a><span class="pull-right">{{price}}</span></p>
                                </div>
                            </div>
                        </div>
                    {{/each}}
                </div>
                <div class="row flex-footer rm-margin-lr" style=" width:100%;">{{>footer}}</div>
                    
          
            </div>
         
        </div>
        <!-- /#page-content-wrapper -->

    </div>
    

